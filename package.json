{
  "name": "publisher-portal",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "VUE_APP_UI_VERSION=$npm_package_version VUE_APP_CHAPERONE_SERVER_URL='https://chaperone-rsm.devnet.holotest.net/' vue-cli-service serve",
    "serve:mock": "VUE_APP_UI_VERSION=$npm_package_version VUE_APP_MOCK_WEB_SDK=true vue-cli-service serve",
    "serve:local": "VUE_APP_UI_VERSION=$npm_package_version VUE_APP_CHAPERONE_SERVER_URL='http://localhost:24274' vue-cli-service serve",
    "build": "VUE_APP_UI_VERSION=$npm_package_version VUE_APP_CHAPERONE_SERVER_URL='https://chaperone-rsm.holo.host/' vue-cli-service build",
    "build:devnet": "VUE_APP_UI_VERSION=$npm_package_version VUE_APP_CHAPERONE_SERVER_URL='https://chaperone-rsm.devnet.holotest.net/' vue-cli-service build",
    "build:QA": "VUE_APP_UI_VERSION=$npm_package_version VUE_APP_CHAPERONE_SERVER_URL='https://qa-chaperone-rsm.holotest.net/' vue-cli-service build",
    "lint": "vue-cli-service lint",
    "storybook": "start-storybook -p 6006",
    "deploy-storybook": "yarn build-storybook && gh-pages -d storybook-static -u 'github-actions-bot <support+actions@github.com>'",
    "build-storybook": "build-storybook",
    "test": "DEBUG_PRINT_LIMIT=10000 yarn jest --watch --runInBand --detectOpenHandles",
    "test:ci": "yarn jest --runInBand --detectOpenHandles --passWithNoTests",
    "hc:nix-shell": "nix-shell ../holo-hosting-app-rsm/default.nix",
    "hc:gen-agent": "hc s clean && hc sandbox generate ../holo-hosting-app-rsm/core-app.happ -a='hha'",
    "hc:run-agent": "hc sandbox -f=4444 r 0 -p=8888",
    "holo:chaperone": "npx chaperone-server --config ./chaperone.json"
  },
  "dependencies": {
    "@holo-host/identicon": "^0.1.0",
    "@holo-host/web-sdk": "^0.6.6-prerelease",
    "@testing-library/user-event": "^13.2.1",
    "add": "^2.0.6",
    "dotenv": "^8.2.0",
    "lodash": "^4.17.20",
    "mitt": "^3.0.0",
    "moment": "^2.29.1",
    "sass": "^1.51.0",
    "sass-loader": "^10",
    "snapshot-diff": "^0.9.0",
    "vue": "^3.2.37",
    "vue-router": "^4.0.16",
    "vue-select": "^4.0.0-beta.3",
    "vue-slider-component": "^4.0.0-beta.4",
    "vuex": "^4.0.1",
    "waait": "^1.0.5"
  },
  "devDependencies": {
    "@holo-host/chaperone": "^9.0.0",
    "@storybook/addon-a11y": "^6.5.6",
    "@storybook/addon-actions": "^6.5.6",
    "@storybook/addon-essentials": "^6.5.6",
    "@storybook/addon-interactions": "^6.5.6",
    "@storybook/addon-links": "^6.5.6",
    "@storybook/builder-webpack5": "^6.5.6",
    "@storybook/manager-webpack5": "^6.5.6",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/testing-library": "^0.0.11",
    "@storybook/vue3": "^6.5.6",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/user-event": "^13.2.1",
    "@testing-library/vue": "^6.4.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/test-utils": "^2.0.0-alpha.8",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.0.0-0",
    "gh-pages": "^4.0.0",
    "jest": "^26.5.3",
    "jest-canvas-mock": "^2.4.0",
    "storybook-addon-mock": "^2.4.0",
    "storybook-i18n": "^1.0.11",
    "storybook-vue3-router": "^2.2.1",
    "typescript": "^4.0.3",
    "vue-jest": "^5.0.0-alpha.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
